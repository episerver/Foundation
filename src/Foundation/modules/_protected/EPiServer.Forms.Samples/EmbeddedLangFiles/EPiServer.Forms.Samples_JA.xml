    <enums>
      <episerver>
        <forms>
          <samples>
            <criteria>
              <submissionstatus>
                <hassubmitted>送信済み</hassubmitted>
                <hasnotsubmitted>未送信</hasnotsubmitted>
              </submissionstatus>

              <fieldvaluecomparecondition>
                <equals>次と等しい</equals>
                <notequals>次と等しくない</notequals>
                <contains>次を含む</contains>
                <notcontains>次を含まない</notcontains>
              </fieldvaluecomparecondition>
            </criteria>
          </samples>
        </forms>
      </episerver>
    </enums>

    <episerver>
      <forms>
        <samples>
          <criteria>
            <formcriterion>
              <category>Episerverフォーム</category>
              <displayname>送信済みフォーム</displayname>
              <description>訪問者によるフォームの送信状態を確認します。</description>
              <submissionstatus>送信ステータス</submissionstatus>
              <selectedform>選択されたフォーム</selectedform>
            </formcriterion>

            <submittedvaluecriterion>
              <category>Episerverフォーム</category>
              <displayname>送信済みフォーム値</displayname>
              <description>訪問者によるフォーム送信値を確認します。</description>
              <selectedform>選択されたフォーム</selectedform>
              <selectedfield>選択されたフィールド</selectedfield>
              <condition>条件</condition>
              <value>値</value>
            </submittedvaluecriterion>
          </criteria>

          <datetimepickertype>
            <datepicker>日付ピッカー</datepicker>
            <timepicker>時刻ピッカー</timepicker>
            <datetimepicker>日時ピッカー</datetimepicker>
          </datetimepickertype>

          <confirmationdialog>
            <title>サマリー</title>
            <buttons>
              <ok>OK</ok>
              <cancel>キャンセル</cancel>
            </buttons>
          </confirmationdialog>

          <editview>
            <requirejs>reCAPTCHAを表示するには、JavaScriptが必要です。</requirejs>
          </editview>
          <messages>
            <imagesubmitbutton>
              <initializationmessage>これは画像送信ボタンです。画像を選択してください。</initializationmessage>
            </imagesubmitbutton>
          </messages>
        </samples>
        <viewmode>
          <addresselement>
            <addresslabel>住所</addresslabel>
            <streetlabel>番地</streetlabel>
            <citylabel>市町村</citylabel>
            <statelabel>都道府県</statelabel>
            <postallabel>郵便番号</postallabel>
            <countrylabel>国</countrylabel>
          </addresselement>
        </viewmode>
        <validators>
          <episerver.forms.samples.implementation.validation.datetimevalidator>
            <displayname>日時検証ツール</displayname>
            <message>有効な日付と時刻を{0} hh:mmフォーマットで入力してください。</message>
          </episerver.forms.samples.implementation.validation.datetimevalidator>
          <episerver.forms.samples.implementation.validation.datevalidator>
            <displayname>日付検証ツール</displayname>
            <message>有効な日付を{0}フォーマットで入力してください。</message>
          </episerver.forms.samples.implementation.validation.datevalidator>
          <episerver.forms.samples.implementation.validation.timevalidator>
            <displayname>時刻検証ツール</displayname>
            <message>午前10:20など、有効な時刻を入力してください。</message>
          </episerver.forms.samples.implementation.validation.timevalidator>
          <episerver.forms.samples.implementation.validation.recaptchavalidator>
            <displayname>再キャプチャ検証ツール</displayname>
            <message>無効なキャプチャ値。</message>
          </episerver.forms.samples.implementation.validation.recaptchavalidator>
          <episerver.forms.implementation.validation.datetimerangevalidator>
            <displayname>日付と時刻の範囲</displayname>
            <message>有効な日付と時刻の範囲を入力してください。</message>
          </episerver.forms.implementation.validation.datetimerangevalidator>
          <episerver.forms.implementation.validation.addressvalidator>
            <displayname>住所</displayname>
            <message>有効な住所を入力してください。</message>
          </episerver.forms.implementation.validation.addressvalidator>
        </validators>
      </forms>
    </episerver>
